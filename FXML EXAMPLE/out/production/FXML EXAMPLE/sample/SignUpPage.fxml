<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SignUpPageController">
   <children>
      <TextField fx:id="employeeName" layoutX="193.0" layoutY="58.0" promptText="Name" />
      <TextField fx:id="username" layoutX="151.0" layoutY="118.0" promptText="username" />
      <TextField fx:id="password" layoutX="164.0" layoutY="167.0" promptText="password" />
      <ComboBox fx:id="department" layoutX="163.0" layoutY="221.0" prefWidth="150.0" promptText="department" />
      <Button fx:id="signUp" layoutX="224.0" layoutY="318.0" mnemonicParsing="false" onAction="#onSignUpClicked" text="sign up" />
      <Label fx:id="promptText" layoutX="224.0" layoutY="359.0" text="Label" />
      <Button layoutX="418.0" layoutY="143.0" mnemonicParsing="false" onAction="#goToLoginPage" text="log in" />
   </children>
</AnchorPane>
